{
  "__sample": "https://palantir.github.io/tslint/usage/configuration/",
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "quotemark": [
      "single"
    ],
    "indent": [
      true,
      "spaces",
      4
    ],
    "no-var-requires": true,
    "no-empty": false
  },
  "rulesDirectory": []
}